<!DOCTYPE html>
<html>
  <head>
    <title>User list</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>User list</h1>
    <ul>
        <%
            var list = '';
            for (i = 0; i < userlist.length; i++) {
                list += '<li><a href="mailto:' + userlist[i].email + '">'+ userlist[i].username + '</a></li>';
            }
        %>
        <%- list %>
    </ul>
    <a href=\>Back to home</a>
  </body>
</html>
